```js !plugin type=viewer of=matter
const styles = {
  root: {
    xdisplay: "flex",
    gap: "0.6em",
  },
  item: {
    marginRight: "0.4em",
  },
};

export const viewer = ({ node, React }) => {
  const matter = lit.file.data.frontmatter;
  const keys = Object.keys(matter);

  const Item = ({ id }) => (
    <span key={id} style={styles.item}>
      <span>{id}</span>: <span>{matter[id]}</span>
    </span>
  );

  return (
    <div style={styles.root}>
      {keys.map((key) => (
        <Item id={key} />
      ))}
    </div>
  );
};

