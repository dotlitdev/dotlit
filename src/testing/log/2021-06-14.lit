```js
const mLon = 40075017
const mLat = 40007863
const x = 25
const n = Math.pow(2, x)
const dLon = 360 / mLon / n
const dLat = 180 / mLat / n

const geo2hash = (lat,lon) => {
  return `${Math.floor((lat+90)/dLat).toString(16)}x${Math.floor((lon+180)/dLon).toString(16)}`
}
const hash2geo = hash => {
  const [hLat,hLon] = hash.split('x')
  const lat = (parseInt(hLat,16) * dLat) - 90
  const lon = (parseInt(hLon,16) * dLon) - 180 
  return {mLon, mLat, x, n, dLon, dLat, lat, lon}
}

return [geo2hash(30,30), hash2geo('64xaf')]
```
