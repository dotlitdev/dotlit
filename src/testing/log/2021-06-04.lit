    // Try to accomodate known CORS proxy implementations:
    // - https://jcubic.pl/proxy.php?  <-- uses query string
    // - https://cors.isomorphic-git.org  <-- uses path
    const corsProxify = (corsProxy, url) =>
      corsProxy.endsWith('?')
        ? `${corsProxy}${url}`
        : `${corsProxy}/${url.replace(/^https?:\/\//, '')}`