```jsx !plugin type=viewer of=matter
export const viewer = ({node,React}) => {
  const matter = lit.file.data.frontmatter
  const keys = Object.keys(matter)
  return <div>{keys.map(key => <span key={key}><span>{key}</span>: <span>{matter[key]}</span></span>)}</div>
}
