```js
return (async fn=>{
  const {default: Fuse} = await import('https://cdn.jsdelivr.net/npm/fuse.js@6.4.6/dist/fuse.esm.js')
  const fuse = new Fuse(books, {
  keys: ['title', 'author.firstName']
})

// 3. Now search!
return fuse.search('jon')
})()
