```js !plugin id=example-menu type=menu
export const menu = (ctx, {React, Menu}) => {
  const rc = React.createElement
  return rc( Menu, {
    title:"Example",
    disabled: false,
  }, [rc('span', {onClick: () => alert("Hi From example plugin!")}, 'Menu Item')])
}
```