                                            "meta": "./utils/unist-util-position-to-node.js",
                                            "value": "export const positionToNode = (pos,tree) => {\n\n  const start = pos.start\n    ? pos.start\n    : pos.line && pos.column\n      ? pos\n      : undefined\n\n  const end = pos.end\n    ? pos.end\n    : pos.line && pos.column\n      ? pos\n      : undefined\n\n  if (tree.children) {\n\n  } if (tree.position.start.line >= start.line && tree.position.end.line <= end.line) {\n    return tree\n  }\n}",
